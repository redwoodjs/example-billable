{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "billable",
  "main": "src/worker.tsx",
  "placement": {
    "mode": "smart"
  },
  "routes": [
    {
      "pattern": "billable.me",
      "custom_domain": true
    }
  ],
  "compatibility_date": "2025-08-21",
  "compatibility_flags": ["nodejs_compat"],
  "assets": {
    "binding": "ASSETS"
  },
  "observability": {
    "enabled": true
  },
  "durable_objects": {
    "bindings": [
      {
        "name": "SESSION_DURABLE_OBJECT",
        "class_name": "SessionDurableObject"
      },
      {
        "name": "APP_DURABLE_OBJECT",
        "class_name": "AppDurableObject"
      }
    ]
  },
  "vars": {
    "APP_URL": "https://billable.me",
    "APP_NAME": "billable",
    "WEBAUTHN_APP_NAME": "billable"
  },
  "migrations": [
    {
      "tag": "v2",
      "new_classes": ["SessionDurableObject"],
      "new_sqlite_classes": ["AppDurableObject"]
    }
  ],
  "r2_buckets": [
    {
      "binding": "R2",
      "bucket_name": "billable"
    }
  ]
}
